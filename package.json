{
  "name": "@nebula.js/sn-map",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.8.0",
  "description": "An agnostic map",
  "license": "MIT",
  "author": "QlikTech International AB",
  "keywords": [
    "qlik",
    "nebula",
    "stardust"
  ],
  "files": [
    "api-specifications",
    "core",
    "dist",
    "sn-map.js",
    "sn-map.qext",
    "sn-map-ext"
  ],
  "engines": {
    "node": ">=8"
  },
  "main": "dist/sn-map.js",
  "scripts": {
    "build": "node ./scripts/build.js --core --ext",
    "build:watch": "node ./scripts/build.js --ext --mode development -w",
    "lint": "eslint --ext .ts,.js src/",
    "start": "nebula serve",
    "sense": "nebula sense",
    "types:check": "tsc --noEmit",
    "types:watch": "tsc --noEmit -w",
    "test:unit": "jest --env=jsdom --runInBand src/",
    "test:rendering": "playwright test",
    "test:local:rendering": "./test/rendering/scripts/run-rendering-test.sh",
    "test:local:update:screenshots": "./test/rendering/scripts/update-screenshots.sh",
    "prepublishOnly": "rm -rf dist && yarn run build",
    "spec": "scriptappy-from-jsdoc -c ./spec-configs/props.conf.js",
    "format": "prettier --write .",
    "major-release": "git pull && yarn version --message 'chore: v%s' --major && git push --follow-tags",
    "minor-release": "git pull && yarn version --message 'chore: v%s' --minor && git push --follow-tags",
    "patch-release": "git pull && yarn version --message 'chore: v%s' --patch && git push --follow-tags",
    "pre-release": "git pull && yarn version --prerelease --message 'chore: v%s' --preid='pre' && git push --follow-tags",
    "version": "yarn spec && git add api-specifications",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "7.12.13",
    "@babel/preset-typescript": "^7.14.5",
    "@jest/types": "^27.0.6",
    "@material-ui/core": "^4.11.4",
    "@nebula.js/cli": "3.0.4",
    "@nebula.js/cli-build": "3.0.4",
    "@nebula.js/cli-sense": "3.0.4",
    "@nebula.js/cli-serve": "3.0.4",
    "@playwright/test": "1.24.2",
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/enzyme": "3.10.11",
    "@types/jest": "^27.0.3",
    "@types/node": "^18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "@types/requirejs": "^2.1.31",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.6",
    "axios": "^1.1.3",
    "babel-jest": "^27.0.5",
    "enzyme": "3.11.0",
    "eslint": "5.12.1",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.15.0",
    "eslint-plugin-mocha": "5.2.1",
    "eslint-plugin-prettier": "3.4.0",
    "fs-extra": "8.1.0",
    "husky": "^8.0.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.3",
    "prettier": "2.3.0",
    "pretty-quick": "^3.1.3",
    "qlik-chart-modules": "^0.13.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "scriptappy-from-jsdoc": "0.7.0",
    "typescript": "^3.9.10"
  },
  "peerDependencies": {
    "@nebula.js/stardust": ">=3.0.4"
  }
}
